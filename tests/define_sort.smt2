(define-sort Set (T) (Array T Bool))
(define-sort IList () (List Int))
(define-sort List-Set (T) (Array (List T) Bool))
(define-sort I () Int)
(declare-fun s1 () (Set I))
(declare-fun s2 () (List-Set Int))
(declare-fun a () I)
(declare-fun l () IList)
(assert (= (select s1 a) true))
(assert (= (select s2 l) false))
(check-sat)
(get-info :model)